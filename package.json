{
  "name": "writeme",
  "version": "0.0.0",
  "description": "A markdown engine to write pretty and interactive documentations and posts.",
  "private": true,
  "repository": "https://github.com/g4rcez/writeme.git",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "start": "turbo run start"
  },
  "devDependencies": {
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "0.3.0",
    "turbo": "^1.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@7.16.0",
  "volta": {
    "node": "18.12.1"
  },
  "dependencies": {
    "next": "13.0.4"
  }
}
