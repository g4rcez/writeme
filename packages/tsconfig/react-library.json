{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "alwaysStrict": true,
    "strict": true,
    "downlevelIteration": true,
    "lib": [
      "ESNext",
      "DOM.Iterable",
      "DOM"
    ],
    "module": "ESNext",
    "target": "es6"
  }
}
